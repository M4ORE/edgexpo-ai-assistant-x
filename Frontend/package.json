{
  "name": "edgexpo-ai-assistant-x",
  "version": "1.0.13",
  "description": "AI-powered desktop assistant for exhibition and business use with voice conversation, business card recognition, and intelligent interaction features",
  "main": "src/main.js",
  "author": "EdgExpo Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/M4ORE/edgexpo-ai-assistant-x"
  },
  "bugs": {
    "url": "https://github.com/M4ORE/edgexpo-ai-assistant-x/issues"
  },
  "homepage": "https://github.com/M4ORE/edgexpo-ai-assistant-x#readme",
  "keywords": [
    "ai",
    "assistant",
    "electron",
    "vue",
    "voice-recognition",
    "business-card",
    "desktop-app",
    "exhibition",
    "conversation"
  ],
  "scripts": {
    "dev:vue": "vite",
    "dev:electron": "electron .",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run dev:vue\" \"npm run dev:electron\"",
    "build:vue": "vite build",
    "build": "cross-env NODE_ENV=production npm run build:vue && electron-builder",
    "build2": "electron-builder --dir",
    "preview": "vite preview"
  },
  "dependencies": {
    "@mdi/font": "^7.0.0",
    "adm-zip": "^0.5.16",
    "axios": "^1.7.9",
    "better-sqlite3": "^12.2.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.1",
    "echarts-gl": "^2.0.9",
    "electron-rebuild": "^3.2.9",
    "express": "^4.17.1",
    "fs-extra": "^11.3.0",
    "iconv-lite": "^0.7.0",
    "jieba-js": "^1.0.2",
    "mammoth": "^1.10.0",
    "marked": "^15.0.8",
    "ml-matrix": "^6.12.1",
    "multer": "^1.4.5-lts.2",
    "music-metadata": "^11.2.1",
    "nanoid": "^3.3.4",
    "natural": "^8.1.0",
    "node-fetch": "^2.7.0",
    "pdf-parse": "^1.1.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "stopword": "3.1.4",
    "uuid": "^11.1.0",
    "vue": "^3.3.0",
    "vue-axios": "^3.5.2",
    "vue-echarts": "^7.0.3",
    "vue-i18n": "^9.14.2",
    "vue-router": "^4.5.0",
    "vuetify": "^3.3.0",
    "ws": "^8.13.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "concurrently": "^8.0.0",
    "cross-env": "^7.0.3",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "jest": "^30.0.5",
    "supertest": "^7.1.4",
    "vite": "^4.0.0",
    "wait-on": "^7.0.1"
  },
  "build": {
    "appId": "com.edgexpo.ai-assistant-x",
    "productName": "EdgExpo AI Assistant X",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "dist/**/*",
      "src/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true
    }
  }
}
